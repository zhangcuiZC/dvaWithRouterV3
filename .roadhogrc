{
  "entry": "src/index.js",
  "hash": true,
  "theme": {
    "@border-radius-base": "1px"
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }],
        ["module-resolver", {
          "alias": {
            "dva": "dva-react-router-3"
          }
        }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }],
        ["module-resolver", {
          "alias": {
            "dva": "dva-react-router-3"
          }
        }]
      ]
    }
  },
  "proxy": {
  }
}
